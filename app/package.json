{
    "name": "app",
    "private": true,
    "version": "0.0.0",
    "type": "module",
    "scripts": {
        "dev": "vite",
        "build": "tsc -b && vite build",
        "lint": "biome check src",
        "lint:fix": "biome check --write src",
        "format": "biome format --write src",
        "test": "vitest --reporter=verbose",
        "test:coverage": "vitest run --coverage",
